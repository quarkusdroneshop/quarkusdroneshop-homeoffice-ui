{
  "name": "quarkuscoffeeshop-homeoffice-ui",
  "version": "0.1.5",
  "description": "UI for Quarkus Coffeeshop Homeoffice",
  "main": "server.js",
  "repository": "https://github.com/quarkuscoffeeshop/quarkuscoffeeshop-homeoffice-ui.git",
  "homepage": "https://github.com/quarkuscoffeeshop/quarkuscoffeeshop-homeoffice-ui",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node server.js",
    "start:dev": "webpack serve --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.20",
    "@types/react-router-dom": "^5.1.7",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.2",
    "css-minimizer-webpack-plugin": "*",
    "dotenv-webpack": "^8.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.1.0",
    "imagemin": "^7.0.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^1.3.7",
    "prettier": "^2.2.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sb": "^9.1.0-alpha.3",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^8.0.17",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@patternfly/react-charts": "^7.2.0",
    "@patternfly/react-core": "^4.278.1",
    "@patternfly/react-icons": "^4.9.2",
    "@patternfly/react-styles": "^4.8.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-last-location": "^2.0.1",
    "react-use": "^17.6.0",
    "recharts": "^2.15.3",
    "victory-core": "^36.9.2"
  }
}
